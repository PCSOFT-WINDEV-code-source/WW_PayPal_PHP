   -
     name : SetLangue
     procedure_id : 2053109257921812098
     type_code : 14
     code : |1-
      // Résumé : Définit la langue pour le site PayPal 
      // Syntaxe :
      //SetLangue (<nLangue>)
      //
      // Paramètres :
      //	nLangue : Langue à utiliser
      // Valeur de retour :
      // 	Aucune
      
      Procédure publique SetLangue(nLangue)
      
      SELON nLangue
      	CAS langueAnglais : gsLanguage 	= "GB"    //Anglais
      	CAS langueEspagnol : gsLanguage = "ES" 	//Espagnol
      	CAS langueFrançais : gsLanguage = "FR" 	//Français
      	CAS langueChinois : gsLanguage = "CN" 	//Chinois
      FIN
      
      
      _HTML()
     type : 458752
