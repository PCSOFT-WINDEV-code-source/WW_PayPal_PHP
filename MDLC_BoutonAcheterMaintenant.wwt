   -
     name : _CREATEFORMVALUE
     internal_properties : CAAAAAgAAAC+jxFIHySEQZJgtYwLETewgPv290JpfSBNLbQw/OkoveoRcZtb9SXw7Sz9XSQIlj/YiXWSCjpSNYDmq896Pfjz7qTY5bu5ZGMcV/0yVo2GGwh1UItUv4hZSyNwN1YNbqXwXcuK9drTCm+ymgFl
     procedure_id : 1902560101413289375
     type_code : 14
     code : |1-
      // Résumé : Créer un champ caché de formulaire
      // Syntaxe :
      //[ <Résultat> = ] _CREATEFORMVALUE (<sNom>, <vValeur> [, <bUniquementSiDifférentValeurDéfaut>])
      //
      // Paramètres :
      //	sNom : Nom du champ
      //	vValeur : Valeur du champ
      //	bUniquementSiDifférentValeurDéfaut (valeur par défaut=0) : Vrai si le code HTML ne doit être généré que si la valeur est différente de la valeur par défaut
      // Valeur de retour :
      // 	chaîne : Chaîne HTML
      
      Procédure restreinte _CREATEFORMVALUE(sNom, local vValeur, bUniquementSiDifférentValeurDéfaut = faux)
      	
      	si bUniquementSiDifférentValeurDéfaut alors
      		si vValeur dans ("", 0) alors
      			renvoyer ""
      		FIN
      	fin
      
      RENVOYER ChaîneConstruit("<input type=""hidden"" name=""%1"" value=""%2"">", sNom, vValeur)		
     type : 458752
   -
     name : _HTML
     internal_properties : 
     procedure_id : 1902560101413355161
     type_code : 14
     code : |1-
      // Résumé : Génère le code HTML nécessaire pour l'exécution du bouton de paiement PayPal
      // Syntaxe :
      //_HTML ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure restreinte _HTML()
      
      // Bulle
      sAlt est une chaine = (gsLanguage <>nationFrançais ? "Make payments with PayPal - it’s fast, free and secure!" sinon "Effectuez vos paiements via PayPal : une solution rapide, gratuite et sécurisée !")
      
      //
      // Valeurs obligatoires
      //
      si gsAccount = "" alors gsAccount = pPayPal.gsPayPalAccount
      sAccount est une chaine = _CREATEFORMVALUE("business", gsAccount)
      
      // Montant
      sMontant est une chaine = _CREATEFORMVALUE("amount", gmoAmount)
      
      // Nom et référence de l'objet
      sItemName est une chaine = _CREATEFORMVALUE("item_name", gsItemName)
      sItemNmbr est une chaine = _CREATEFORMVALUE("item_number", gsItemNumber)
      
      // Quantités
      sQuantité est une chaine = _CREATEFORMVALUE("quantity", gnQuantity)
      sModQuantité est une chaine = _CREATEFORMVALUE("undefined_quantity", gbUndefQuantity, vrai)
      
      // Monnaie
      sMonnaie est une chaine = _CREATEFORMVALUE("currency_code", gsCurrencyCode, vrai)
      
      //
      // Valeurs optionnelles
      //
      
      // Pages de retour
      sPageAnnule est une chaine = _CREATEFORMVALUE("cancel_return", gsCancelReturn, vrai)
      sPageOK est une chaine = _CREATEFORMVALUE("return", gsReturn, vrai)
      sTxtContinuer est une chaine = _CREATEFORMVALUE("cbt", gsCbt, vrai)
      
      
      // Commentaires
      sCn	est une chaine = _CREATEFORMVALUE("cn", gsCn, vrai)
      sNoNote est une chaine = _CREATEFORMVALUE("no_note", gbNoNote, vrai)
      
      // Mise en page
      sHdImage est une chaine = _CREATEFORMVALUE("cpp_header_image", gsCppHeaderimage, vrai)
      sHdColor est une chaine = _CREATEFORMVALUE("cpp_headerback_color", gsCppHeaderBackColor, vrai)
      sHdBdColor est une chaine = _CREATEFORMVALUE("cpp_headerborder_color", gsCppHeaderBorderColor, vrai)
      sPayFlow est une chaine = _CREATEFORMVALUE("cpp_payflow_color", gsCppPayFlowColor, vrai)
      sCs est une chaine = _CREATEFORMVALUE("cs", gbCs, vrai)
      sLogo est une chaine = _CREATEFORMVALUE("image_url", gsImageUrl, vrai)
      
      // Adresses de livraison
      sShipping est une chaine = _CREATEFORMVALUE("no_shipping", gnNoShipping, vrai)
      
      // Mode de retour
      sRM est une chaine = _CREATEFORMVALUE("rm", gnRm, vrai)
      
      // Divers
      sCustom est une chaine = _CREATEFORMVALUE("custom", gsCustom, vrai)
      sHandling est une chaine = _CREATEFORMVALUE("handling", gmoHandling, vrai)
      sInvoice est une chaine = _CREATEFORMVALUE("invoice", gsInvoice, vrai)
      sShipping01 est une chaine = _CREATEFORMVALUE("shipping", gmoShipping, vrai)
      sShipping02 est une chaine = _CREATEFORMVALUE("shipping2", gmoShipping2, vrai)
      sID est une chaine = "PAYPAL" + DonneIdentifiant()
      
      // Complète
      sHTML est une chaine = [
      <html><body topmargin=0 leftmargin=0>
      <form name="form%1" action="https://www.sandbox.paypal.com/cgi-bin/webscr" method="post" target="_parent">
      <input type="hidden" name="cmd" value="_xclick">
      <input type="image" src="%2" alt="%3" onclick="form%1.submit();">
      <input type="hidden" name="lc" value=%4>
      <input type="hidden" name="bn" value="PP-BuyNowBF">		
      <img alt="" border="0" src="https://www.sandbox.paypal.com/fr_FR/i/scr/pixel.gif" width="1" height="1">
      %5
      </form></body></html>
      ]
      
      
      sHTML = chaineconstruit(sHTML, sID, gsButton, sAlt,gsLanguage, sAccount + ...
      sMontant + ...
      sItemName + ...
      sItemNmbr + ...
      sQuantité + ...
      sModQuantité + ...
      sMonnaie + ...
      sPageAnnule + ...
      sPageOK + ...
      sTxtContinuer + ...
      sCn + ...
      sLogo + ...
      sHdImage + ...
      sHdColor + ...
      sHdBdColor + ...
      sCs + ...
      sPayFlow + ...
      sShipping + ...
      sRM + ...
      sCustom + ...
      sHandling + ...
      sInvoice + ...
      sShipping01 + ...
      sShipping02)
      
      
      // Mode test ?
      si pas pPayPal.gbModeTest alors sHTML = remplace(sHTML, "sandbox.", "")
      // Langue
      si gsLanguage <> nationFrançais alors sHTML = remplace(sHTML, "fr_FR", "en_US")
      
      // Affichage du code HTML
      gsHTML = sHTML
      HTM_PAYPAL = "<iframe src=""" + remplace( PageAdresse(""), "http:", "" ) + "?&WD_BUTTON_CLICK_=" + BTN_SOURCE..Alias + """ border=0 frameborder=0 style=""width:auto;height:auto;overflow:none;""></iframe>"
