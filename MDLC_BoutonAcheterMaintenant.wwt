   -
     name : SetPageRetourPaiementAnnulé
     procedure_id : 1902560114299374203
     type_code : 14
     code : |1+
      // Résumé : Définit la page de retour à appeler lors de l'annulation du paiement
      // Syntaxe :
      //SetPageRetourPaiementAnnulé (<sPageRetour>)
      //
      // Paramètres :
      //	sPageRetour : Nom de la page WEBDEV ou adresse URL de la page à afficher
      // Valeur de retour :
      // 	Aucune
      
      Procédure publique SetPageRetourPaiementAnnulé(sPageRetour)
      
      	gsPageRetourAnnule = sPageRetour
      
      	si gsPageRetourAnnule = "" alors
      		gsCancelReturn = ""
      	sinon
      		gsCancelReturn = PageAdresse("", paCOMPLETE) + "?WD_BUTTON_CLICK_="  + BTN_RETOURANNULE..alias
      	FIN
      	
      	_HTML()
      	
     type : 458752
   -
     name : SetPageRetourPaiementOK
     procedure_id : 1902560118594407222
     type_code : 14
     code : |1-
      // Résumé : Définit la page de retour à appeler lors de l'acceptation du paiement
      // Syntaxe :
      //SetPageRetourPaiementOK (<sPageRetour> [, <sTexteBoutonContinuer>])
      //
      // Paramètres :
      //	sPageRetour : Nom de la page WEBDEV ou adresse URL de la page à afficher
      //	sTexteBoutonContinuer (valeur par défaut="") : Texte du bouton 'Continuer' sur le page de paiement de PayPal
      // Valeur de retour :
      // 	Aucune
      
      Procédure publique SetPageRetourPaiementOK(sPageRetour, sTexteBoutonContinuer = "")
      
      gsPageRetourOK = sPageRetour
      
      si gsPageRetourOK <> "" alors
      	gsReturn = PageAdresse("", paCOMPLETE) + "?WD_BUTTON_CLICK_="  + BTN_RETOUROK..alias 
      	gsCbt	 = sTexteBoutonContinuer
      sinon
      	gsReturn = ""
      	gsCbt = ""
      fin
      
      _HTML()
     type : 458752
